{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "ucr-api",
  "sourceRoot": "apps/ucr-api/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/ucr-api",
        "command": "npm run dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "cwd": "apps/ucr-api",
        "command": "npm run build"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/ucr-api",
        "command": "npm run start"
      }
    }
  },
  "tags": []
}
